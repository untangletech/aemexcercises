<div class="mdc-dialog c-image-viewer__dialog" id="js-image-viewer-dialog">
  <div class="mdc-dialog__surface c-image-viewer__dialog-content">
    <section class="mdc-dialog__body c-image-viewer__dialog-body mdc-dialog__body--scrollable">
        <img src="{{this}}" alt="caseImage" />
    </section>
    <footer class="mdc-dialog__footer c-image-viewer__dialog-footer">
      <button type="button" class="mdc-button mdc-dialog__footer__button mdc-dialog__footer__button--accept c-image-viewer__dialog-footer--left js-delete-image-btn"><i class="material-icons mdc-button__icon">delete</i><span class="mdc-button__icon__text">{{i18keyText 'cmms_imageViewer.delete'}}</span></button>
      <button type="button" class="mdc-icon-button material-icons mdc-dialog__footer__button mdc-dialog__footer__button--cancel c-image-viewer__dialog-footer--right">done</button>
    </footer>
  </div>
  <div class="mdc-dialog__backdrop"></div>
</div>